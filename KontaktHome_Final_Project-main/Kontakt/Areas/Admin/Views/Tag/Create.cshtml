

@model Tag
@{ ViewData["Title"] = "Create"; }

<div class="row">
    <div class="col-lg-12 h3 mb-4 text-gray-800">
        <h1>Brand Create</h1>
    </div>
    <div class="col-lg-6 mx-auto box-form">
        <form method="post" enctype="multipart/form-data">

            <div class="form-group">
                <label asp-for="Name"></label>
                <input asp-for="Name" class="form-control" placeholder="Enter Tag Name">
                <span class="text-danger" asp-validation-for="Name"></span>
            </div>
            <div class="d-flex justify-content-between">
                <a class="btn btn-primary" asp-action="Index" asp-route-status="@ViewBag.Status" asp-route-page="@ViewBag.PageIndex">Back To List</a>
                <button type="submit" class="btn btn-success">Create</button>
            </div>
        </form>

    </div>
</div>

@section Script{
    <partial name="_ValidationPartial">


}






