                    <figure class="styles_iconWrap-figure">
                        <img class="styles_iconWrap-img" src="~/User/assets/img/setting/accountInfo_2.svg" alt="icon">
                    </figure>
                    @if (ViewBag.OrderCount > 0)
                    {
                        <span class="styles_iconLabel">@ViewBag.OrderCount</span>
                    }

                </div>
                <span class="styles_link-text">
                    Mənim sifarişlərim
                </span>
            </button>
        </a>
    </li>

