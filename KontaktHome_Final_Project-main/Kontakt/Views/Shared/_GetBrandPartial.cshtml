@model List<CategoryBrand>
@if (Model.Count() > 0)
{
    <div class="brand-list active">
        <div class="brand-box">
            <ul class="brands">
                @foreach (var item in Model)
                {
                    <li class="brand-item">
                        <a class="brand-item-btn" asp-controller="product" asp-action="ProductList" asp-route-id="@item.Id" asp-route-keyfrom="brand">
                            <div class="brand-item-img">
                                <img src="~/user/assets/img/brandImg/@item.ImageUrl" alt="">
                            </div>
                            <span class="brand-item-text">@item.Brand.Name</span>
                        </a>
                    </li>
                }

            </ul>
        </div>
    </div>
}


