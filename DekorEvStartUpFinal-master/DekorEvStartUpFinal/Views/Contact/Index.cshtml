@model Setting
@{
    ViewData["Title"] = "Index";
}



    <section id="Contact">
        <div class="container">
            <div class="row mb-5">
                <div class="col-lg-4">
                    <div class="box">
                        <img src="~/assets/images/contact_phone.png" alt="">
                        <a href="tel:0502656463">@Model.PhoneNumber</a>
                    </div>
                </div>
                <div class="col-lg-4">
                    <div class="box">
                        <img src="~/assets/images/mail.png" alt="">
                        <a href="mailto:@Model.Email">@Model.Email</a>
                    </div>
                </div>
                <div class="col-lg-4">
                    <div class="box">
                        <img src="~/assets/images/placeholder.png" alt="">
                        <span>@Model.Adress</span>
                    </div>
                </div>
            </div>

            <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3039.428490145618!2d49.85175681468929!3d40.37719496596823!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x40307d079efb5163%3A0xc20aa51a5f0b5e01!2sCode%20Academy!5e0!3m2!1sen!2s!4v1651053725073!5m2!1sen!2s" width="100%" height="450" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
        </div>
    </section>
